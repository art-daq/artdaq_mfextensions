<?xml version="1.0"?>

<!-- "units" may be "milliseconds", "seconds", "minutes", "hours", "MB" and "GB" -->

<RCConfig version="default">

  <DetectorId> 0 </DetectorId>
  <RCLogToDB enabledFlag="false"/>
  <Krb5Id></Krb5Id>
  <Krb5IdSfx></Krb5IdSfx>
  <Krb5Keytab></Krb5Keytab>
  <SimMode> 0 </SimMode>

  <SSTOTime units="seconds" size="30" /> <!-- only seconds and minutes are allowed -->

  <RunDuration units="minutes" size="1440" />  <!-- 24 hours -->
  <SRDuration units="minutes" size="60" />
  <HeartRate units="seconds" size="2" />
  <RunType>Test</RunType>
  <Verbosity> 1 </Verbosity>
  <Comment></Comment>
  <ResourceList/>
</RCConfig>
